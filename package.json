{
  "name": "react-infinite-calendar",
  "version": "2.3.1",
  "description": "Infinite scrolling date-picker built with React, with localization, themes, keyboard support, and more.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd",
    "styles.css"
